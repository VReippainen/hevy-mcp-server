{
  "name": "@vreippainen/hevy-mcp-server",
  "version": "0.0.2",
  "description": "A MCP server for Hevy",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "bin": "./dist/index.js",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/VReippainen/hevy-mcp-server.git"
  },
  "keywords": [
    "hevy",
    "mcp",
    "fitness",
    "workout",
    "model-context-protocol"
  ],
  "author": "Ville Reippainen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/VReippainen/hevy-mcp-server/issues"
  },
  "homepage": "https://github.com/VReippainen/hevy-mcp-server#readme",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "inspect": "pnpm build && npx @modelcontextprotocol/inspector@latest node dist/index.js",
    "start": "node dist/index.js",
    "build": "tsc",
    "prepublishOnly": "pnpm build",
    "publish:prod": "pnpm publish",
    "publish:beta": "pnpm publish --tag beta",
    "publish:alpha": "pnpm publish --tag alpha",
    "publish:rc": "pnpm publish --tag rc",
    "version:beta": "pnpm version prerelease --preid=beta",
    "version:alpha": "pnpm version prerelease --preid=alpha",
    "version:rc": "pnpm version prerelease --preid=rc",
    "version:patch": "pnpm version patch",
    "version:minor": "pnpm version minor",
    "version:major": "pnpm version major",
    "test": "vitest run --silent",
    "test:watch": "vitest --silent",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.7.0",
    "axios": "^1.8.4",
    "axios-cache-interceptor": "^1.7.0",
    "dotenv": "^16.3.1",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/node": "^22.13.13",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "@vitest/coverage-v8": "^1.6.1",
    "eslint": "^9.23.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.5",
    "eslint-plugin-unused-imports": "^4.1.4",
    "prettier": "^3.5.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "vitest": "^1.6.1"
  }
}
